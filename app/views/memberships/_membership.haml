%tr
  %td= link_to(membership.community&.to_label, membership.community) if membership.community
  %td= link_to(membership.team&.to_label, membership.team) if membership.team
  %td= link_to(membership.gamer&.to_label, membership.gamer) if membership.gamer
  %td= link_to(membership.role&.to_label, membership.role) if membership.role
  %td= membership.type
  %td= yes_or_no membership.active
  %td= membership.privacy_label
  %td= membership.status_label
  %td= membership.created_at
  %td= membership.updated_at
  %td
    = render partial: 'shared/crud_actions', locals: { model: membership }
